<?xml version="1.0" encoding="UTF-8"?>
<configuration status="info">
    <Appenders>
        <Console name="STDOUT" target="SYSTEM_OUT">
            <PatternLayout pattern="%-5p %d %-5p [%t] %C{2} (%F:%L) - %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <logger name="org.apache.xml.security.stax.impl.util.SignerOutputStream" level="info"/>
        <logger name="org.apache.xml.security.stax.impl.util.DigestOutputStream" level="info"/>
        <logger name="org.apache.xml.security.utils.DigesterOutputStream" level="info"/>
        <logger name="org.apache.xml.security.utils.SignerOutputStream" level="info"/>
        <logger name="org.apache.xml.security.encryption.XMLCipher" level="info"/>
        <logger name="org.apache.xml" level="info"/>
        <logger name="org.apache.xml.security.stax.impl.InputProcessorChainImpl" level="info"/>
        <logger name="org.apache.xml.security.stax.impl.OutputProcessorChainImpl" level="info"/>

        <Root level="debug">
            <AppenderRef ref="STDOUT"/>
        </Root>

    </Loggers>
</configuration>
